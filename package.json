{
  "name": "akat-microservice",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build": "tsc && npm run copy-html && npm run copy-locales",
    "copy-html": "copyfiles -u 1 src/html/**/* dist/",
    "copy-locales": "copyfiles -u 1 src/locales/**/* dist/",
    "start": "node dist/index.js",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "lint": "eslint . --ext .js,.ts,.mjs,.cjs",
    "lint:fix": "eslint . --ext .js,.ts,.mjs,.cjs --fix",
    "prettier": "prettier --write .",
    "test:ts": "npx tsc",
    "seed": "npx ts-node src/seed/seed.ts",
    "play": "npx ts-node src/auto-use-session.ts",
    "play2": "npx ts-node src/auto-use-sessionV2.ts",
    "play3": "npx ts-node src/auto-use-sessionV3.ts",
    "play4": "npx ts-node src/auto-use-sessionV4.ts",
    "play5": "npx ts-node src/auto-use-sessionV5.ts",
    "play6": "npx ts-node src/auto-use-sessionV6.ts",
    "play7": "npx ts-node src/auto-use-sessionV7.ts",
    "login": "npx ts-node src/login.ts",
    "mail": "npx ts-node src/controllers/autoTakeVerify.controller.ts",
    "prod": "docker compose -f docker-compose.prod.yml --env-file .env.production up -d --build",
    "develop": "docker compose -f docker-compose.yml --env-file .env up -d --build",
    "down": "docker compose down -v --remove-orphans"
  },
